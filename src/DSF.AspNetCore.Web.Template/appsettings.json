{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "AllowedHosts": "*",

  "Dsf.Authentication": {
    "LoginUrl": "/Account/LogIn",
    "SignedOutRedirectUri": "https://localhost:44319/",
    "Scopes": "openid cegg_profile dsf.submission",
    "RedirectUri": "https://localhost:44319/signin-oidc",
    "ClientSecret": "secret",
    "ClientId": "Oidc.Client.AuthorizationCode",
    "Authority": "https://aztest.cyprus.gov.cy/cylogin/core",
    "ExpireTimeSpanInMinutes": "30",
    "AuthCookieName": "DsfCyLoginAuthCookie"
  },

  "client-key": "12345678901234567890123456789000", //Client key will be given for each service
  "ApiUrl": "https://dsf-api-dev.dmrid.gov.cy/", //staging url https://dsf-api-dev.dmrid.gov.cy/
  "MatomoID": "9" //set the  Matomo ID, in case you yould like your service to send information about your service
}


